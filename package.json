{
  "name": "bogie",
  "version": "0.0.6",
  "description": "Collision extension for PIXI.js",
  "main": "src/index.js",
  "np": {
    "tests": false
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "fix": "eslint --fix ./src",
    "dev": "webpack-dev-server",
    "build": "NODE_ENV=production webpack --mode=production",
    "docs": "NODE_ENV=production webpack --config ./webpack.config.docs.js --mode=production && node ./renderDocs.js",
    "patch": "npm run build && np patch",
    "minor": "npm run build && np minor",
    "major": "npm run build && np major"
  },
  "keywords": [
    "pixi",
    "physics"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Sleepy-Fish/BogieJS.git"
  },
  "author": "J Riley Harrington <jrileyharrington@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "doxdox": "^3.0.0",
    "doxdox-parser-sleepyfish": "git://github.com/Sleepy-Fish/doxdox-parser-sleepyfish.git#master",
    "doxdox-plugin-bogie": "git://github.com/Sleepy-Fish/doxdox-plugin-bogie.git#master",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "np": "^5.2.1",
    "prismjs": "^1.19.0",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "pixi.js": "^5.2.0"
  },
  "dependencies": {
    "pixi.js": "^5.2.1"
  }
}
