{
  "name": "bogie",
  "version": "0.0.7",
  "description": "Collision extension for PIXI.js",
  "main": "src/index.js",
  "np": {
    "anyBranch": false,
    "cleanup": true,
    "tests": true,
    "publish": true,
    "contents": ".",
    "releaseDraft": true
  },
  "scripts": {
    "test": "mocha --require @babel/register --preset babel ./test",
    "fix": "eslint --fix ./src",
    "dev": "webpack-dev-server",
    "build": "NODE_ENV=production webpack --mode=production && npm run docs",
    "docs": "node ./generateDocs.js",
    "patch": "npm run build && np patch",
    "minor": "npm run build && np minor",
    "major": "npm run build && np major"
  },
  "keywords": [
    "pixi",
    "physics",
    "geometry",
    "collision",
    "engine",
    "game",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Sleepy-Fish/BogieJS.git"
  },
  "author": "J Riley Harrington <jrileyharrington@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "doxdox": "^3.0.0",
    "doxdox-parser-sleepyfish": "git://github.com/Sleepy-Fish/doxdox-parser-sleepyfish.git#master",
    "doxdox-plugin-json": "git://github.com/Sleepy-Fish/doxdox-plugin-json.git#master",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^16.2.2",
    "mocha": "^7.1.1",
    "np": "^5.2.1",
    "prismjs": "^1.19.0",
    "terser-webpack-plugin": "^2.3.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "peerDependencies": {
    "pixi.js": "^5.2.0"
  },
  "dependencies": {
    "pixi.js": "^5.2.1"
  }
}
